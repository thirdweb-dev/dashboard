  "pnpm": {
    "overrides": {
      "bn.js": "^5.2.1",
      "zod": "^3.19.1",
      "axios@<0.28": "^0.28.0"
    }
  },
  "next-unused": {
    "alias": {},
    "include": [
      "components",
      "hooks",
      "schema",
      "theme",
      "utils",
      "@3rdweb-sdk",
      "constants",
      "tw-components",
      "contract-ui"
    ],
    "exclude": [
      "node_modules",
      "types",
      "tw-components"
    ],
    "entrypoints": []
  },
  "browserslist": [
    "defaults",
    "unreleased versions",
    "not UCAndroid > 0"
  ]
}
